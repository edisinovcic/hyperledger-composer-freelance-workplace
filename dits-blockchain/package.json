{
  "engines": {
    "composer": "^0.20.5"
  },
  "name": "dits",
  "version": "0.1.11",
  "description": "Dits network",
  "scripts": {
    "lint": "eslint 'lib/**/*.js' 'test/**/*.js'",
    "test:unit": "nyc --reporter=html --reporter=text mocha -t 0 test/*Spec.js",
    "test:bdd": "cucumber-js",
    "test:all": "npm run test:unit && npm run test:bdd",
    "test:all:all": "npm run test:all",
    "prepush": "npm run list && nom run test:all",
    "archive:create": "mkdirp ./code && composer archive create --sourceType dir --sourceName . -a ./code/dits.bna",
    "local:install": "composer network install -a ./code/dits.bna --card PeerAdmin@hlfv1",
    "local:start": "composer network start --networkName dits --networkVersion $npm_package_version --networkAdmin admin --networkAdminEnrollSecret adminpw --card PeerAdmin@hlfv1 --file admin.card",
    "local:upgrade": "composer network upgrade --card PeerAdmin@hlfv1 --networkName dits -V $npm_package_version",
    "local:rest-server": "composer-rest-server --card admin --namespaces never --p 3000"
  },
  "keywords": [
    "composer",
    "composer-network"
  ],
  "author": "dits",
  "email": "edi.sinovcic@gmail.com",
  "license": "Apache-2.0",
  "devDependencies": {
    "composer-admin": "^0.20.4",
    "composer-cli": "^0.20.4",
    "composer-client": "^0.20.4",
    "composer-common": "^0.20.4",
    "composer-connector-embedded": "^0.20.4",
    "composer-cucumber-steps": "^0.20.4",
    "chai": "latest",
    "chai-as-promised": "latest",
    "cucumber": "^2.2.0",
    "eslint": "latest",
    "nyc": "latest",
    "mkdirp": "latest",
    "mocha": "latest"
  },
  "dependencies": {
    "passport-github": "^1.1.0"
  }
}
